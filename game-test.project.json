{
	"name": "game-test",
	"globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
	"tree": {
		"$className": "DataModel",
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"Vault": {
				"$path": "out",
				"include": {
					"$path": "include",
					"node_modules": {
						"$className": "Folder",
						"@rbxts": {
							"$path": "node_modules/@rbxts"
						}
					}
				}
			}
		},

		"ServerScriptService": {
			"$className": "ServerScriptService",
			
			"main": {
				"$path": "scripts/run-test.server.luau"
			}
		}
	}
}